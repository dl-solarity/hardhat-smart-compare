{
  "name": "@dlsl/hardhat-smart-compare",
  "version": "0.1.0",
  "description": "This package facilitates contract upgradability and provides various comparison tools.",
  "scripts": {
    "install": "npx husky install",
    "compile": "tsc --build .",
    "build-local": "npm run compile && npm run lint-fix && bash ./scripts/build-local.sh",
    "clean": "bash ./scripts/clean.sh",
    "test": "mocha --recursive \"test/**/*.ts\" --exit",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test ",
    "lint-fix": "prettier --write \"./**/*.ts\"",
    "publish-to-npm": "npm run compile && npm run lint-fix && bash ./scripts/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dl-solidity-library/hardhat-smart-compare.git"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin",
    "distributedlab"
  ],
  "author": "Distributed Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dl-solidity-library/hardhat-smart-compare/issues"
  },
  "homepage": "https://github.com/dl-solidity-library/hardhat-smart-compare#readme",
  "dependencies": {
    "hardhat": "^2.10.0",
    "lodash": "^4.17.21",
    "solidity-ast": "^0.4.49"
  },
  "peerDependencies": {
    "hardhat": "^2.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-like": "^1.1.1",
    "@types/fs-extra": "^11.0.0",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-like": "^1.1.1",
    "fs-extra": "^11.1.0",
    "husky": "^7.0.2",
    "lodash.isequal": "^4.5.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sinon": "^15.1.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^8.1.0",
    "typescript": "~4.5.2"
  }
}
